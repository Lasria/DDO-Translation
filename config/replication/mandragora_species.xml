<replication>
  <message_set name="mandragora species name">
    <match>^ui\\00_message\\mandragora\\mandragora_species_name$</match>
    <rewrite>
      <original_match>^【(.+)】(.+)・(.+)$</original_match>
      <translation_replace>({0}) {1} - {2}</translation_replace>
      <translation_arg index="0" message_set="mandragora type" />
      <translation_arg index="1" message_set="mandragora variant" />
      <translation_arg index="2" message_set="mandragora type" />
    </rewrite>
    <rewrite>
      <original_match>^【(.+)】(.+)$</original_match>
      <translation_replace>({0}) {1}</translation_replace>
      <translation_arg index="0" message_set="mandragora type" />
      <translation_arg index="1" message_set="mandragora variant" required="true" />
    </rewrite>
    <rewrite>
      <original_match>^【(.+)】(.+)$</original_match>
      <translation_replace>({0}) {1}</translation_replace>
      <translation_arg index="0" message_set="mandragora type" />
      <translation_arg index="1" message_set="mandragora species name" />
    </rewrite>
    <rewrite>
      <original_match>^(.+)・(.+)$</original_match>
      <translation_replace>{0} - {1}</translation_replace>
      <translation_arg index="0" message_set="mandragora variant" />
      <translation_arg index="1" message_set="mandragora type" />
    </rewrite>
    <rewrite>
      <original_match>^(.+)$</original_match>
      <translation_replace>{0}</translation_replace>
      <translation_arg message_set="mandragora variant" required="true" />
    </rewrite>
  </message_set>
