<replication>
  <message_set name="npc class">
    <match>^ui\\00_message\\npc\\npc_class_name$</match>
    <match>^ui\\00_message\\npc\\func_class_name$</match>
    <match>^ui\\00_message\\npc\\func_select_name$</match>
    <rewrite>
      <original_match>^ジョブマスター：(.+)$</original_match>
      <translation_replace>Job Master: {0}</translation_replace>
      <translation_arg message_set="job name" />
    </rewrite>
    <rewrite>
      <original_match>^グランドミッション：(.+)$</original_match>
      <translation_replace>Grand Mission: {0}</translation_replace>
      <translation_arg message_set="quest name" required="true" />
    </rewrite>
    <rewrite>
      <original_match>^グランドミッション：(.+)$</original_match>
      <translation_replace>Grand Mission: {0}</translation_replace>
    </rewrite>
    <rewrite>
      <original_match>^ウォーミッション：(.+)$</original_match>
      <translation_replace>War Mission: {0}</translation_replace>
      <translation_arg message_set="location name" required="true" />
    </rewrite>
    <rewrite>
      <original_match>^ウォーミッション：(.+)$</original_match>
      <translation_replace>War Mission: {0}</translation_replace>
    </rewrite>
  </message_set>
</replication>